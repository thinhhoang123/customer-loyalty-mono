{
  "name": "share-assets",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/assets/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/assets",
        "main": "libs/assets/src/index.ts",
        "tsConfig": "libs/assets/tsconfig.lib.json",
        "assets": [
          "libs/assets/*.md"
        ]
      }
    },
    "svgr": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "svgr --typescript -d libs/assets/src/lib libs/assets/src/assets"
        ]
      }
    }
  }
}